{
	"compilerOptions": {
		"target": "ES5",
		"outDir": "../../public/js/",
		"declaration": true,
		"declarationMap": true,
		"sourceMap": true,
		"removeComments": true,
		"strict": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"strictFunctionTypes": true,
		"strictPropertyInitialization": true,
		"noImplicitThis": true,
		"alwaysStrict": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"lib": [
			"dom",
			"es2015"
		]
	},
	"include": [
		"contact.ts"
	]
}
